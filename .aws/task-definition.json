[
    {
      "name": "${container_name}",
      "image": "${repository_url}",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ],
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "project.settings"
        },
        {
          "name": "SECRET_KEY",
          "value": "${django_secret_key}"
        },
        {
          "name": "DEBUG",
          "value": "${django_debug}"
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": "${django_allowed_hosts}"
        },
        {
          "name": "DB_ENGINE",
          "value": "${django_db_engine}"
        },
        {
          "name": "DB_NAME",
          "value": "${django_db_name}"
        },
        {
          "name": "DB_USER",
          "value": "${django_db_user}"
        },
        {
          "name": "DB_PASS",
          "value": "${django_db_pass}"
        },
        {
          "name": "DB_HOST",
          "value": "${django_db_host}"
        },
        {
          "name": "DB_PORT",
          "value": "${django_db_port}"
        }
      ]
    }
  ]